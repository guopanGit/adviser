/** app.wxss **/

view {
    box-sizing: border-box;
    line-height: 1;
}


/* flex*/

.flex {
    display: flex;
    align-content: center;
    justify-content: space-between;
}

/* canvas 样式 */
.container {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
} 